{
  "path": {
    "src": {
      "srcPath": "./app",
      "components": "/_components",
      "html": [
        "/**/*.html"
      ],
      "style": [
        "/assets/css/main.scss"
      ],
      "script": [
        "/assets/js/custom.js"
      ],
      "image": [
        "/assets/img/**/*.*"
      ],
      "font": [
        "/assets/fonts/**/*.*"
      ]
    },
    "dist": {
      "distPath": "./dist",
      "html": "/",
      "style": "/assets/css/",
      "script": "/assets/js/",
      "image": "/assets/img/",
      "font": "/assets/fonts/"
    }
  }
}